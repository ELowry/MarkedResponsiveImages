{
	"name": "marked-responsive-images",
	"version": "0.3.0",
	"description": "A robust marked extension for handling responsive images with srcset generation.",
	"type": "module",
	"repository": {
		"type": "git",
		"url": "https://github.com/ELowry/MarkedResponsiveImages.git"
	},
	"bugs": {
		"url": "https://github.com/ELowry/MarkedResponsiveImages/issues"
	},
	"homepage": "https://github.com/ELowry/MarkedResponsiveImages#readme",
	"source": "src/index.js",
	"main": "./dist/index.cjs",
	"module": "./dist/index.module.js",
	"unpkg": "./dist/index.umd.js",
	"exports": {
		".": {
			"import": "./dist/index.module.js",
			"require": "./dist/index.cjs"
		}
	},
	"scripts": {
		"build": "microbundle",
		"dev": "microbundle watch",
		"lint": "eslint src",
		"format": "prettier --write \"src/**/*.js\"",
		"check-format": "prettier --check \"src/**/*.js\"",
		"prepublishOnly": "npm run build"
	},
	"files": [
		"dist",
		"src"
	],
	"keywords": [
		"marked",
		"markdown",
		"responsive-images",
		"srcset",
		"extension"
	],
	"author": "Your Name",
	"license": "MIT",
	"peerDependencies": {
		"marked": ">=4.0.0"
	},
	"devDependencies": {
		"eslint": "^9.39.2",
		"eslint-config-prettier": "^10.1.8",
		"microbundle": "^0.15.1",
		"prettier": "^3.8.1",
		"prettier-plugin-css-order": "^2.2.0"
	}
}
